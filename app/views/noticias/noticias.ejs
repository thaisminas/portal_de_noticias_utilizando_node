<!DOCTYPE html>
	<html lang="pt-br">
		<head>
			<meta charset="utf-8"/>
			<title>Notícias</title>
		</head>
	<body>

		<h1>Notícias</h1>

		<table>
			<tr>
				<td>ID</td>
				<td>Titulo</td>
			</tr>

			<% for(var i = 0; i < noticias; i++){ %>
				<tr>
					<td><%= noticias[i].id_noticias %></td>
					<td><%= noticias[i].titulo %></td>
					<td><%= noticias[i].noticias %></td>
				</tr>
			<% } %>
		</table>

	</body>
</html>